        "lint": "jshint --reporter checkstyle . > linting.xml",
        "local-lint": "jshint .",
        "cypress": "cypress run --browser electron --headed",
        "a11y-tests": "cypress run --spec \"cypress/integration/a11y.spec.js\""
    },
